label live-@FLAVOUR@-destruct
	menu label PhoneBo.cx ^Auto Install (Destructive)
	menu default
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@ wipeall

label live-@FLAVOUR@
	menu label ^PhoneBo.cx Live
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label PhoneBo.cx (^Failsafe)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@
